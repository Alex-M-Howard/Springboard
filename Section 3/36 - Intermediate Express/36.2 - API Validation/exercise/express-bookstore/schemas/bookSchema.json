{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "isbn",
    "amazon_url",
    "author",
    "language",
    "pages",
    "publisher",
    "title",
    "year"
  ],
  "properties": {
    "isbn": {
      "type": "string",
      "default": "",
      "title": "The isbn Schema",
      "examples": ["978-1955904049"]
    },
    "amazon_url": {
      "type": "string",
      "default": "",
      "title": "The amazon_url Schema",
      "examples": [
        "https://www.amazon.com/Sorrow-Festival-Erin-Slaughter/dp/1955904049"
      ]
    },
    "author": {
      "type": "string",
      "default": "",
      "title": "The author Schema",
      "examples": ["Erin Slaughter"]
    },
    "language": {
      "type": "string",
      "default": "",
      "title": "The language Schema",
      "examples": ["English"]
    },
    "pages": {
      "type": "integer",
      "default": 0,
      "title": "The pages Schema",
      "examples": [138]
    },
    "publisher": {
      "type": "string",
      "default": "",
      "title": "The publisher Schema",
      "examples": ["CLASH Books"]
    },
    "title": {
      "type": "string",
      "default": "",
      "title": "The title Schema",
      "examples": ["The Sorrow Festival"]
    },
    "year": {
      "type": "integer",
      "default": 0,
      "title": "The year Schema",
      "examples": [2022]
    }
  },
  "examples": [
    {
      "isbn": "978-1955904049",
      "amazon_url": "https://www.amazon.com/Sorrow-Festival-Erin-Slaughter/dp/1955904049",
      "author": "Erin Slaughter",
      "language": "English",
      "pages": 138,
      "publisher": "CLASH Books",
      "title": "The Sorrow Festival",
      "year": 2022
    }
  ]
}
